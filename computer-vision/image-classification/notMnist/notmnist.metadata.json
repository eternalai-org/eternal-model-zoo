{
    "input": {
      "type": "image",
      "transforms": [
        {
          "type": "grayscale",
          "parameters": {
            "enabled": true
          }
        },
        {
          "type": "resize",
          "parameters": {
            "width": 28,
            "height": 28
          }
        },
        {
          "type": "normalize",
          "parameters": {
            "method": "minmax",
            "range": {
              "min": 0,
              "max": 1
            }
          }
        }
      ]
    },
    "output": {
      "type": "classification",
      "parameters": {
        "labels": [
          "A",
          "B",
          "C",
          "D",
          "E",
          "F",
          "G",
          "H",
          "I",
          "J"
        ]
      }
    }
  }